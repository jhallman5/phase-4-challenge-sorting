<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Vinyl</title>

  </head>
  <body>
    <h1><%- include header %></h1>
    <h1>Vinyl</h1>
    <div class="button">
      <a href ="/sign-in">Sign In</a>
      <a href ="/sign-up">Sign Up</a>

      <div class="album-list">
        <% albums.forEach(function(album) { %>
          <div>
            <h5><a href="/albums/<%= album.id %>"><%= album.title %></a></h5>
            <span><%= album.artist %></span>
          </div>
        <% }) %>
      </div>
      
    </div>
  </body>
</html>
