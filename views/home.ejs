<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Vinyl</title>

  </head>
  <body>
    <h1><%- include('header.ejs') %></h1>

    <h1>Vinyl</h1>
    <div class="button">

    <h2>Records!</h2>
      <div class="album-list">
        <% albums.forEach(function(album) { %>
          <div>
            <h5><a href="/albums/<%= album.id %>"><%= album.title %></a></h5>
            <span>by <%= album.artist %></span>
          </div>
        <% }) %>
      </div>
    <h2>Recent Reviews</h2>

    </div>
  </body>
</html>
